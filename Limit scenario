
def size(cms):
    if cms < 38:
        return 'S'
    elif cms > 38 and cms < 42:
        return 'M'
    else:
        return 'L'


assert(size(37) == 'S') # Less than 38
assert(size(40) == 'M') # Between 38 and 42
assert(size(43) == 'L') # greater than 42

#Exactly 38 and 42
assert(size(42) == 'L')
assert(size(38) == 'M')
print("All is well (maybe!)")
